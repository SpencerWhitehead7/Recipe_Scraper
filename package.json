{
  "name": "plate-scraper",
  "version": "1.0.0",
  "description": "An app for collecting, editing, organizing, and storing recipes",
  "main": "server/index",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx --ignore-pattern /public/*",
    "lint-fix": "eslint . --ext .js,.jsx --fix --ignore-pattern /public/*",
    "clean": "rm -rf built dist node_modules && npm install",
    "seed": "NODE_ENV='script' npx ts-node server/scripts/seed.ts",
    "cleanupOrphanedTags": "NODE_ENV='script' npx ts-node server/scripts/cleanupOrphanedTags.ts",
    "build": "npx tsc && webpack -p",
    "start": "node built",
    "dev:be": "ts-node-dev server",
    "dev:fe": "node built & webpack-dev-server -d --hot",
    "dev": "webpack-dev-server -d --hot & ts-node-dev server",
    "test:be": "NODE_ENV='test' mocha -r ts-node/register --file server/mochaSetup.ts --timeout 10000 --exit -- 'server/**/__tests__/*.spec.ts'",
    "heroku-postbuild": "webpack -p"
  },
  "author": "spencer",
  "license": "Unlicense",
  "dependencies": {
    "@babel/runtime": "^7.10.1",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/cheerio": "^0.22.18",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/node": "^13.13.9",
    "@types/passport": "^1.0.3",
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "class-validator": "^0.12.2",
    "classnames": "^2.2.6",
    "connect-typeorm": "^1.1.3",
    "core-js": "^3.6.5",
    "css-loader": "^3.5.3",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "focus-trap-react": "^6.0.0",
    "normalize.css": "^8.0.1",
    "passport": "^0.4.1",
    "pg": "^8.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^5.7.2",
    "react-hot-loader": "^4.12.21",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.2",
    "typeorm": "^0.2.25",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/mocha": "^7.0.2",
    "@types/supertest": "^2.0.9",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.2.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "supertest": "^4.0.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "volleyball": "^1.5.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
